{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "no-unused-vars": "warn",
        "no-undef": "error",
        "quotes": "off"
    }
}
